{
  "name": "utiljs",
  "version": "0.0.6",
  "description": "TSUMIKI INC. JavaScript utilities",
  "repository": {
    "type": "git",
    "url": "git@github.com:tsumikiinc/utiljs.git"
  },
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "test",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "mocha --compilers js:espower-babel/guess test/**/*.js",
    "lint:es6": "eslint src/",
    "lint:coffee": "coffeelint -f coffeelint.json src/",
    "lint": "npm ru lint:es6 && npm ru lint:coffee",
    "compile:es6": "babel -d lib/ src/",
    "compile:coffee": "coffee -c -o lib/ src/",
    "compile": "npm ru compile:es6 && npm ru compile:coffee",
    "replace-b:es6": "replace 'import \\$ from '\\''jquery'\\'';' 'const $ = global.$;' src -r",
    "replace-b:coffee": "replace '\\$ = require '\\''jquery'\\''' '$ = global.$' src -r",
    "replace-b": "npm ru replace-b:es6 && npm ru replace-b:coffee",
    "replace-n:es6": "replace 'const \\$ = global.\\$;' 'import $ from '\\''jquery'\\'';' src -r",
    "replace-n:coffee": "replace '\\$ = global.\\$' '$ = require '\\''jquery'\\''' src -r",
    "replace-n": "npm ru replace-n:es6 && npm ru replace-n:coffee",
    "standalone": "browserify index.js -t babelify -t coffeeify -s Util | derequire > build/util.js",
    "minify": "uglifyjs build/util.js -m -c warnings=false --comments '/\\/*!/' -o build/util.min.js",
    "build": "npm ru lint && npm ru replace-b && npm ru compile && npm ru standalone && npm ru minify && npm ru replace-n && npm ru compile"
  },
  "engines": {
    "node": "0.12.x"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babelify": "^6.2.0",
    "browserify": "^11.0.1",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "coffeelint": "^1.11.1",
    "derequire": "^2.0.2",
    "eslint": "^1.2.1",
    "espower-babel": "^3.3.0",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "replace": "^0.3.0",
    "uglify-js": "^2.4.24"
  },
  "peerDependencies": {
    "jquery": ">=1"
  }
}
