{
  "name": "jquery-module",
  "version": "1.0.0",
  "description": "TSUMIKI INC. jQuery modules",
  "repository": {
    "type": "git",
    "url": "git@github.com:tsumikiinc/jquery-module.git"
  },
  "author": "TSUMIKI INC.",
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "test",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "test": "mocha --compilers js:espower-babel/guess test/**/*.spec.js",
    "lint:es6": "eslint src",
    "lint:coffee": "coffeelint -f coffeelint.json src",
    "lint": "npm run lint:es6 && npm run lint:coffee",
    "compile:es6": "babel -d lib src",
    "compile:coffee": "coffee -c -o lib src",
    "compile": "npm run compile:es6 && npm run compile:coffee",
    "replace-b:es6": "replace 'import \\$ from '\\''jquery'\\'';' 'const $ = global.$;' src -r",
    "replace-b:coffee": "replace '\\$ = require '\\''jquery'\\''' '$ = global.$' src -r --include='*.coffee'",
    "replace-b:es3": "replace 'var \\$ = require\\('\\''jquery'\\''\\);' 'var $ = global.$;' src -r",
    "replace-b": "npm run replace-b:es6 && npm run replace-b:coffee && npm run replace-b:es3",
    "replace-n:es6": "replace 'const \\$ = global.\\$;' 'import $ from '\\''jquery'\\'';' src -r",
    "replace-n:coffee": "replace '\\$ = global.\\$' '$ = require '\\''jquery'\\''' src -r --include='*.coffee'",
    "replace-n:es3": "replace 'var \\$ = global.\\$;' 'var $ = require('\\''jquery'\\'');' src -r",
    "replace-n": "npm run replace-n:es6 && npm run replace-n:coffee && npm run replace-n:es3",
    "standalone": "browserify index.js -t babelify -t coffeeify -s \\$Module -p licensify | derequire > build/jquery-module.js",
    "minify": "uglifyjs build/jquery-module.js -m -c warnings=false --comments '/This header is generated by licensify/' -o build/jquery-module.min.js",
    "build": "npm run lint && npm run replace-b && npm run compile && npm run standalone && npm run minify && npm run replace-n && npm run compile"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "coffee-script": "^1.10.0",
    "coffeeify": "^1.1.0",
    "coffeelint": "^1.12.1",
    "derequire": "^2.0.2",
    "eslint": "^1.5.1",
    "espower-babel": "^3.3.0",
    "jsdom": "^6.5.1",
    "licensify": "^2.0.1",
    "mocha": "^2.3.3",
    "power-assert": "^1.0.1",
    "replace": "^0.3.0",
    "uglify-js": "^2.4.24"
  },
  "peerDependencies": {
    "jquery": ">=1"
  },
  "dependencies": {
    "licensify": "^2.0.1"
  }
}
